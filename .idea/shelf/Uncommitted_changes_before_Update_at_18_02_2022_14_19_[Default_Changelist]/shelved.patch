+package com.example.studenthub;
+
+import java.util.ArrayList;
+import java.util.List;
+
+public class ChatRoom {
+
+    public static String DUMMY = "DUMMY";
+    private String id;
+    private String ownerId;
+    private String secondUserId;
+
+    private List<ChatMessage> chatMessages;
+
+
+    public ChatRoom(String id,String ownerId,String secondUserId,List<ChatMessage> chatMessages) {
+        this.ownerId = ownerId;
+        this.secondUserId = secondUserId;
+        this.chatMessages = chatMessages;
+        this.id = id;
+    }
+    public ChatRoom() {}
+    public ChatRoom(String id,String ownerId,String secondUserId) {
+        this.ownerId = ownerId;
+        this.secondUserId = secondUserId;
+        this.chatMessages = new ArrayList<>();
+        chatMessages.add(new ChatMessage(DUMMY,ownerId,secondUserId,DUMMY));
+        this.id = id;
+    }
+
+    public String getOwnerId() {
+        return ownerId;
+    }
+
+    public void setOwnerId(String ownerId) {
+        this.ownerId = ownerId;
+    }
+
+    public String getSecondUserId() {
+        return secondUserId;
+    }
+
+    public void setSecondUserId(String secondUserId) {
+        this.secondUserId = secondUserId;
+    }
+
+    public List<ChatMessage> getChatMessages() {
+        return chatMessages;
+    }
+
+    public void setChatMessages(List<ChatMessage> chatMessages) {
+        this.chatMessages = chatMessages;
+    }
+
+    @Override
+    public String toString() {
+        return "ChatRoom{" +
+                "ownerId='" + ownerId + '\'' +
+                ", secondUserId='" + secondUserId + '\'' +
+                ", chatMessages=" + chatMessages +
+                '}';
+    }
+}
